{
    "name": "netlify-build-fixer",
    "version": "1.0.0",
    "description": "AI-powered tool for diagnosing and fixing Netlify build issues",
    "main": "index.js",
    "bin": {
        "netlify-fixer": "./bin/cli.js"
    },
    "scripts": {
        "start": "node bin/cli.js",
        "test": "jest",
        "lint": "eslint ."
    },
    "keywords": [
        "netlify",
        "build",
        "debugging",
        "deployment",
        "troubleshooting",
        "ci",
        "nextjs",
        "react"
    ],
    "author": "Your Organization",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.6.2",
        "chalk": "^4.1.2",
        "commander": "^11.1.0",
        "conf": "^10.2.0",
        "inquirer": "^8.2.6",
        "log-symbols": "^4.1.0",
        "netlify": "^13.1.10",
        "ora": "^5.4.1",
        "strip-ansi": "^6.0.1",
        "terminal-link": "^2.1.1",
        "yaml": "^2.3.4"
    },
    "devDependencies": {
        "eslint": "^8.53.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/test/**/*.test.js"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "lcov"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/your-org/netlify-build-fixer.git"
    },
    "bugs": {
        "url": "https://github.com/your-org/netlify-build-fixer/issues"
    },
    "homepage": "https://github.com/your-org/netlify-build-fixer"
}